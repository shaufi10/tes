<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grinasia.transport">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name="com.grinasia.transport.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.grinasia.transport.Register"></activity>
        <activity android:name="com.grinasia.transport.AccountActivationActivity"></activity>
        <activity android:name="com.grinasia.transport.MainActivity"></activity>
        <activity android:name="com.grinasia.transport.Detail_Order_Aktif"></activity>
        <activity android:name="com.grinasia.transport.EditProfile"></activity>
    </application>

</manifest>